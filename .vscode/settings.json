{
    "cmake.configureSettings": {
        "CMAKE_TOOLCHAIN_FILE": "../cmake/arm-gcc-toolchain.cmake",
    },
    "C_Cpp.files.exclude": {
        "version.h.in": true,
    },
    "cmake.outputLogEncoding": "utf8",
    "cmake.configureOnOpen": true,
    "files.associations": {
        "nx_api.h": "c",
        "nx_m460_eth_driver.h": "c",
        "string_view": "c",
        "array": "c",
        "initializer_list": "c",
        "*.tcc": "cpp",
        "functional": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp"
    },
}
